{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Computer Product",
    "type": "object",
    "properties": {
      "url": {
        "type": "string",
        "format": "uri"
      },
      "title": {
        "type": "string"
      },
      "price": {
        "type": "string"
      },
      "processor": {
        "type": "string"
      },
      "gpu": {
        "type": "string"
      },
      "motherboard": {
        "type": "string"
      },
      "ram": {
        "type": "string"
      }
    },
    "required": ["url", "title"]
  }
  